{
    "rules": {
        "indent": [2, 2],
        "quotes": [2, "single"],
        "linebreak-style": [2, "unix"],
        "semi": [2, "always"]
    },
    "env": {
        "browser": false,
        "es6": true,
        "node": true,
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "blockBindings": true,
        "jsx": true,
        "experimentalObjectRestSpread": true
    },
    "plugins": ["react"]
}